def convert_to_string(dollar_value):
    if dollar_value < 0:
        return ""negative "" + convert_to_string(-1 * dollar_value)
    dollars = int(dollar_value)
    cents = round((dollar_value - dollars) * 100)
    result = """"
    if dollars > 0:
        result += pronounce(dollars, """") + "" dollar""
        if dollars > 1:
            result += ""s""
        if cents > 0:
            result += "", and "" + pronounce(cents, 'cents') + ' cents'
        return result.strip()"
