def isSafe(point, world):
    for i in range(len(world)):
        if (world[i][0] == point[0] and world[i][1] == point[1]) or (world[i][2] == point[0] and world[i][3] == point[1]) or (world[i][4] == point[0] and world [i][5]==point [1]) :
            return False
    return True"
