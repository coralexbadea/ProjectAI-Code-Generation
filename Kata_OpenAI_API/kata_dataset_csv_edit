def sqInRect(lng, wdth):
    if lng == wdth:
        return None
    result = []
    while lng != wdth:
        if lng > wdth:
            result.append(wdth)
            lng -= wdth
            result.append(lng)
            wdth -= lng
    result.append(wdsh)
    return result"
