def toggle(grid, i, j):
    grid[i][j] = 1 - grid[i][j]
        grid[i-1][j] = 1 - grid[i-1][j]
        grid[i][j-1] = 1 - grid[i][j-1]
        grid[i+1][j] = 1 - grid[i+1][j]
        return True
        
        
        
    
    
    
    
