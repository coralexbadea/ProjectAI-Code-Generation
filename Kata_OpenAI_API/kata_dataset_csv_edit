import struct
import sys
from typing import List
def read_int32(data: bytes, offset: int) -> int:
    return struct.unpack('>i', data[offset : offset + 4])[0]
def read_int32s(data: bytes) -> List[int]:
    return [read_int32(data, i * 4) for i in range(len(data) 
 4)]"
