def number_of_addresses(start, end):
    start = list(map(int, start.split('.')))
    end = list(map(int, end.split('.')))
    return (end[0] - start[0]) * 256 ** 3 + (end[1] - start[1]) * 256 ** 2 + (end[2] - start[2]) * 256 + (end[3] - start[3])"
