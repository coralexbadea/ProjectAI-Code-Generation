def applesDistribution(apples, boxCapacity, maxResidue):
    n = len(apples)
    m = len(boxCapacity)
    dp = [[0 for i in range(maxResidue + 1)] for j in range(n + 1)]
    for i in range(1, n + 1):
        for j in range(1, maxResidue + 1):
            if apples[i - 1] <= j:
                dp[i][j] = max([dp[i - 1][j], dp[i - 1][j - apples[i - 1]] + apples[i - 1]])
            else:
                dp[i][j] = dp[i-1][j]
