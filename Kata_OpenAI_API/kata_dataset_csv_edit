def is_hyperrectangular(arr):
    if len(arr) == 0: return False
    for i in range(len(arr)):
        if type(arr[i]) != list: return False
        if len(arr[i]) != len(arr): return False
    dim = [len(_) for _ in arr]
        if dim[0] != dim[i]: return False
    
    
        
        
    
