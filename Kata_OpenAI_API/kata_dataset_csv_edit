def evenBeforeFixedElement(sequence, fixedElement):
    if fixedElement not in sequence:
        return -1
    for i in range(len(sequence)):
        if sequence[i] == fixedElement:
            break
        if sequence[i] % 2 == 0:
            print(""even"")"
