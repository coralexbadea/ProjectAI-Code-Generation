def find_max_sock(arr):
    max = 0
    for i in range(len(arr)):
        if arr[i] > max:
            max = arr[i]
    return max
def find_min_sock(arr, sock):
    min = sock + 1
    for i in range(len(arr)):
        if arr[i] < min and arr[i] != 0:
            min = arr[i]
    return min"
