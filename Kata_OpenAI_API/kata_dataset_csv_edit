def encode(string):
    string = ''.join([c for c in string if c.isalnum()])
    size = int(math.ceil(math.sqrt(len(string))))
    matrix = [[' '] * size for _ in range(size)]
    i, j, di, dj = 0, 0, 1, 0
    for k in range(len(string)):
