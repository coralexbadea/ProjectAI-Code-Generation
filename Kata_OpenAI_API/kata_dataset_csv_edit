	arr[0] = 0
	for i in range(1, amount + 1):
	    minCoins = float('inf')
         for j in range(len(denominations)): 
            if denominations[j] <= i: 
                numCoins = arr[i - denominations[j]] + 1;  
                if numCoins < minCoins:  
                    minCoins = numCoins;    
            arr[i] = minCoins;    
            
            
