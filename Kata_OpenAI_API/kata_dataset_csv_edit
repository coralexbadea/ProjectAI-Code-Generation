def get_schemes():
    schemes = []
    for scheme in os.listdir('schemes'):
        if not scheme.endswith('.yaml'): continue
        with open('schemes
{}'.format(scheme)) as f:
            data = yaml.load(f)
        schemes.append({ 'name': data['name'], 'description': data['description'] })
    return schemes"
