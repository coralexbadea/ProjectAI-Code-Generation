class Hex:
    def __init__(self, value):
        self.value = value
    def __add__(self, other):
        return Hex(self.value + other)
    def __str__(self):
        return hex(self.value)
    def toJSON(self):
        return str('""0x' + hex(self.value)[2:] + '""')"
