def boggle_board(board, word):
    for i in range(len(board)):
        for j in range(len(board[i])):
            if board[i][j] == word[0]:
                if dfs_searching_for_word(i, j, board, word): return True
    return False
def dfs_searching_for_word(row, col, board, word):
    if len(word) == 1 and board[row][col] == word: return True
