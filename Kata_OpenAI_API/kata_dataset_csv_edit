def stock_list(listOfArt, listOfCat):
    if len(listOfArt) == 0:
        return ''
    result = []
    for i in range(len(listOfCat)):
        total = 0
        for j in range(len(listOfArt)):
            if listOfCat[i] == listOfArt[j][0]:
                total += int(''.join([k for k in listOfArt[j] if k.isdigit()]))
        result.append('({} : {})'.format(listOfCat[i], total))
    return ' - '.join([x for x in result])"
