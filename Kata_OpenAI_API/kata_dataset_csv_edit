def iq_test(numbers):
    numbers = [int(x) for x in numbers.split()]
    evens = []
    odds = []
    for i, n in enumerate(numbers):
        if n % 2 == 0:
            evens.append((i, n))
            odds.append((i, n))
        return evens[0][0] + 1
        return odds[0][0] + 1"
