def caeser_cipher(s, k):
    result = """"
    for i in range(len(s)):
        char = s[i]
        if (char.isupper()):
            result += chr((ord(char) + k - 65) % 26 + 65)
        else:
            result += chr((ord(char) + k - 97) % 26 + 97)
    return result"
