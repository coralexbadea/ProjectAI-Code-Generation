class User(object):
    def __init__(self, name):
        self.name = name
        self.progress = 0
    def __getattr__(self, key):
        if key == 'rank':
            return self._rank_from_progress()
    def _rank_from_progress(self):
        return len({p for p in xrange(100) if p <= self.progress})"
