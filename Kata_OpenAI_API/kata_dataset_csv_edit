    """"""Return sequence with consecutive duplicates removed.""""""
    result = []
    for i in range(len(seq)):
        if i == 0 or seq[i] != seq[i - 1]:
            result.append((seq[i], 1))
            result[-1] = (result[-1][0], result[-1][1] + 1)
    return [item for item, count in result for i in range(count)]"
