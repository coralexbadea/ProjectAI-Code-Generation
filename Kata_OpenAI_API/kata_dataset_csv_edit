def sequence_classifier(arr):
    if len(arr) == 0: return 'Empty'
    elif all(x == arr[0] for x in arr): return 'Arithmetic'
    elif all((x - arr[0]) % 2 == 0 for x in arr): return 'Geometric (even)'
