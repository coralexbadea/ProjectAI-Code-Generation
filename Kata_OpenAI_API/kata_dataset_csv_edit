def bishop_and_king(bishop, king):
    x1 = ord(bishop[0]) - 96
    y1 = int(bishop[1])
    x2 = ord(king[0]) - 96
    y2 = int(king[1])
    if abs((x2-x1)) == abs((y2-y1)):
        return True
    return False"
