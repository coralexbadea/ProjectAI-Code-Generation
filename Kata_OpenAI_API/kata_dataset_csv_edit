def infix_to_postfix(infix):
    precedence = {'^': 4, '*': 3, '
': 3, '+': 2, '-': 2}
    operators = []
    postfix = ''
    for char in infix:
            postfix += char
            operators.append(char)
                popped_operator = operators.pop()
                    postfix += popped_operator
