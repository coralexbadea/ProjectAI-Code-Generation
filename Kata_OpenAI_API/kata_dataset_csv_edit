def is_eviternity(n):
    n = str(n)
    if len(set([i for i in n])) != 3: return False
    if '8' not in n or '5' not in n or '3' not in n: return False
    eights = 0; fives = 0; threes = 0
    for i in range(len(n)):
        if int(n[i]) == 8: eights += 1 
        elif int(n[i]) == 5: fives += 1 
        elif int(n[i]) == 3: threes += 1 
    return eights >= fives and fives >= threes"
