def harmonic_mean(n):
    divisors = [1]
    for i in range(2, int(n**0.5)+1):
        if n % i == 0:
            divisors.append(i)
            if i != n 
                divisors.append(n
i)
    return sum([1
d for d in divisors])
    
    
    
        
        
        
