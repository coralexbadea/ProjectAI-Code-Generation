def even_odd(n):
    even = []
    odd = []
    for i in str(n):
        if int(i) % 2 == 0:
            even.append(i)
        else: 
            odd.append(i)
    return int(''.join([str(x) for x in sorted([int(''.join([str(y) for y in even])), int(''.join([str(z) for z in odd]))], reverse=True)]))"
