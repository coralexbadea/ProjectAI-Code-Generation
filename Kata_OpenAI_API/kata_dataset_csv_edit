def primeFactors(n):
    i = 2
    factors = []
    while i * i <= n:
        if n % i:
            i += 1
            factors.append((i,1))
            n 
= i
        factors.append((n,1))
    return ''.join('({}{})'.format(p,'**{}'.format(e) if e > 1 else '') for p,e in factors).replace(')(',')*(')"
