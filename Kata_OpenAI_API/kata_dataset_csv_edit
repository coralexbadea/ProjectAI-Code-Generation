def apparently(s):
    if s == '':
        return ''
    if 'and' in s:
        index = s.find('and') + 4
        return s[:index] + 'apparently' + s[index:]
    elif 'but' in s:
        index = s.find('but') + 4
        return  s[:index] + 'apparently' +  s[index:]
    else:
        return  str(s)"
