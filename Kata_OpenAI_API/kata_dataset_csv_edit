def triangle(row):
    if len(row) == 1:
        return row
    new_row = ''
    for i in range(len(row) - 1):
        if row[i] == row[i + 1]:
            new_row += row[i] * 2
        else:
            new_row += 'R' if row[i] == 'G' else 'G'
    return triangle(new_row)"
