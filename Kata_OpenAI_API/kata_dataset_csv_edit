def neighbours(matrix, row, col):
    return [matrix[r][c] for r in range(row-1, row+2) for c in range(col-1, col+2) if 0 <= r < len(matrix) and 0 <= c < len(matrix[0])]
if __name__ == '__main__':
    matrix = [[1, 2], [3, 4]]
    print('Test 1')
    print('Actual:', neighbours(matrix=[[1, 2], [3, 4]], row=0 ,col=1))"
