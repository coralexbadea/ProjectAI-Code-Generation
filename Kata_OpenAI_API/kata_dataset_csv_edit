def min_exchange(arr):
    count = 0
    for i in range(len(arr) - 1):
        if arr[i] == 7:
            continue
        if arr[i] > arr[i + 1]:
            count += 1
    return count
if __name__ == '__main__':
    n = int(input())
    arr = list(map(int, input().rstrip().split()))
    result = min_exchange(arr)"
