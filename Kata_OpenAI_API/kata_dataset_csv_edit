def do_jobs(jobs, bob, jim):
    if len(jobs) == 0:
        return 0
    if jobs[0] > 0:
        return max(bob + do_jobs(jobs[1:], bob, jim), jim + do_jobs(jobs[1:], bob, jim))
    elif jobs[0] < 0:
        return min(-bob + do_jobs(jobs[1:], bob, jim), -jim + do_jobs(jobs[1:]))
