class NaturalNumber:
    def __init__(self, n):
        self.n = n
    def __add__(self, other):
        return NaturalNumber(self.n + other.n)
    def __sub__(self, other):
        return NaturalNumber(self.n - other.n)
    def __mul__(self, other):
        return NaturalNumber(int((float('inf') if self.n == 0 else self.n * other.n)))
    def __truediv__(self, other):"
555a03f259e2d1788c000077," The Basic Problem Given a grid of v vertical roads and h horizontal roads, find the number of ways to go from the top-left intersection to the bottom-right, moving only right or down. For example, a grid of v = 4 and h = 3 would look like this: +-+-","
def num_ways(v, h):
    if v == 0 or h == 0:
        return 1
    return num_ways(v-1, h) + num_ways(v, h-1)
