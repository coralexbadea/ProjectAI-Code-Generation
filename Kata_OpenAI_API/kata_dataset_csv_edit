def timeConversion(s):
    if s[-2:] == ""AM"" and s[:2] == ""12"": 
        return ""00"" + s[2:-2] 
    elif s[-2:] == ""AM"": 
        return s[:-2] 
    elif s[-2:] == ""PM"" and s[:2] == ""12"": 
        return str(int(s.replace(""PM"","""")) - 1200) + str(s.replace(""PM"",""""))  
