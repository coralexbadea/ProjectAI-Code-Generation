def separate_values(input):
    result = {}
    for value in input:
        if type(value) == int:
            if 'int' not in result.keys():
                result['int'] = []
            result['int'].append(value)
        elif type(value) == str:
            if 'str' not in result.keys():
                result['str'] = []
            result['str'].append(value)
        elif type(value) == bool:
            if 'bool' not in result.keys():
                result['bool'] = []
