def area_polygon(coords):
    x = [i[0] for i in coords]
    y = [i[1] for i in coords]
    return round((sum([x[i]*y[i+1]-x[i+1]*y[i] for i in range(len(coords)-1)]) + x[-1]*y[0]-x[0]*y[-1])
