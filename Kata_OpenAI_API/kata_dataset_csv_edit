class ByteStream:
    def __init__(self):
        self.buffer = []
    def flip(self):
        self.read_mode = not self.read_mode
    def write(self, byte):
        if type(byte) is not int:
            raise TypeError(""byte must be an integer"")
        if byte < 0 or byte > 255:
            raise ValueError(""byte must be in range 0 <= byte <= 255"")
