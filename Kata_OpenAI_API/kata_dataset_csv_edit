def timeMath(t1, op, t2):
    t1 = sum([int(x) * 60 ** i for i, x in enumerate(reversed(t1.split(':')))])
    t2 = sum([int(x) * 60 ** i for i, x in enumerate(reversed(t2.split(':')))])
