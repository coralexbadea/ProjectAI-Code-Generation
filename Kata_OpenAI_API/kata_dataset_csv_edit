def index_documents(documents, term, match_exact=False, match_prefix=False):
    result = []
    for i in range(len(documents)):
        if (match_exact and documents[i] == term) or (match_prefix and documents[i].startswith(term)):
            result.append(i+1)
    return sorted(result)"
