def insert_missing_letters(st):
    alphabet = 'abcdefghijklmnopqrstuvwxyz'
    for i in range(len(alphabet)):
        if alphabet[i] not in st:
            st = st.replace(alphabet[i], alphabet[i]) + st
    return st"
