def look_and_say(n):
    if n == 1:
        return '1'
    prev = look_and_say(n-1)
    result = ''
    count = 1
    for i in range(len(prev)):
        if i+1 < len(prev) and prev[i] == prev[i+1]:
            count += 1
    return result"
