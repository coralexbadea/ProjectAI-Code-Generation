def chinese_numeral(number):
    if number == 0: return '零'
    if number < 0: return '负' + chinese_numeral(-number)
    result = ''
    while number > 0:
        quotient, remainder = divmod(number, 10)
        if remainder == 0: result += '零'
        else: result += CHINESE_DIGITS[remainder]
        number = quotient
    return result[::-1]"
