def correct_sentence(text: str) -> str:
    """"""
        returns a corrected sentence which starts with a capital letter and ends with a dot.
    """"""
    if text[0].islower():
        text = text[0].upper() + text[1:]
    if not text.endswith('.'):
        return f'{text}.'
    return text"
