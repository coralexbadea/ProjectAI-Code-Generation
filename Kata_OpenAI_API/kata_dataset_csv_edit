def format_duration(seconds):
    if seconds == 0: return ""now""
    def helper(seconds, result):
        if seconds == 0: return result[:-1]
        years = int(seconds 
 31536000)
        seconds -= years * 31536000
        days = int(seconds 
 86400)
        seconds -= days * 86400
        hours = int(seconds 
 3600)
        seconds -= hours * 3600
        minutes = int(seconds 
 60)
        seconds -= minutes * 60
