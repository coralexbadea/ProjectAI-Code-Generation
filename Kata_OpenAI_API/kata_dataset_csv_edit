def prettyTimeFormat(sec):
    hours = sec 
 3600
    minutes = (sec - hours * 3600) 
 60
    seconds = sec - hours * 3600 - minutes * 60
    return str(hours) + "":"" + str(minutes) + "":"" + str(seconds)
