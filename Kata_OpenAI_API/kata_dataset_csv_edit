def TCP_automaton(string):
    state = 0
    for i in string:
        if state == 0:
            if i == 'a': state = 1
            else: return False
            if i == 'b': state = 2
            elif i == 'c': state = 3
            else: return False
            if i == 'd': return True
            elif i == 'e': return True
            else: return False"
