def is_special_linear_group(matrix):
    if matrix[0][0] * matrix[1][1] == matrix[0][1] * matrix[1][0]:
        return False
    det = (matrix[0][0] * matrix[1][1]) - (matrix[0][1] * matrix[1][0])
    if det != 1:
        return False
    for i in range(2):
        for j in range(2):
