def sum_fracts(lst):
    if not lst: return None
    elif len(lst) == 1: return lst[0]
    numerator = 0
    denominator = 1
    for i in range(len(lst)):
        numerator += (denominator * lst[i][0])
        denominator *= lst[i][1]
    gcd = math.gcd(numerator, denominator)
    if gcd != 0: numerator 
= gcd; denominator 
