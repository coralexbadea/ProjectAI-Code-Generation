def chessKnight(cell):
    x = ord(cell[0]) - 96
    y = int(cell[1])
    moves = 0
    if x + 2 <= 8 and y + 1 <= 8:
        moves += 1
    if x + 2 <= 8 and y - 1 >= 1:
        moves += 1
    if x - 2 >= 1 and y + 1 <= 8:
        moves += 1
    if x - 2 >=1 and y -1 >=1: 
        moves +=1 "
