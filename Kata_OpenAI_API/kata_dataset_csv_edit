def fizz_buzz_reloaded(start, stop):
    result = []
    for i in range(start, stop):
        if i % 3 == 0 and i % 5 == 0: result.append('FizzBuzz')
        elif i % 3 == 0: result.append('Fizz')
        elif i % 5 == 0: result.append('Buzz') 
        else: result.append(i)
    return result"
