class List:
    def __init__(self, *args):
        self.list = [*args]
    def __str__(self):
        return str(self.list)
    def __repr__(self):
        return f'List({"", "".join([str(i) for i in self.list])})'
    def prepend(self, value):
        self.list = [value] + self.list
    def append(self, value):
        self.list += [value]
