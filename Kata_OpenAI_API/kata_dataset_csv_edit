def local_tops(msg):
    msg = list(msg)
    result = []
    for i in range(len(msg)):
        if i == 0:
            result.append(''.join(sorted(set([x for x in msg[i]]), reverse=True)))
        else:
            temp = []
            for j in range(len(result[i-1])):
                    temp += [result[i-1][j]]"
56b27f35c616d3b889000043,Implement Laziness in a Strict Language.,"
from functools import wraps
def lazy(func):
    """"""Decorator for making a function lazy.
    A lazy function returns a generator instead of an actual value. The first
    time the generator is iterated over, the function is executed and the result
    is returned. Subsequent iterations will use the cached result.""""""
    @wraps(func)
    def wrapper(*args, **kwargs):
        gen = func(*args, **kwargs)
