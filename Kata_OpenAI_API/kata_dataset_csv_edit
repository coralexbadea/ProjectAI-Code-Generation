def count_words(text):
    words = text.split()
    return {word: words.count(word) for word in set(words)}
    
print(count_words(""Today, I went to a local business, and they were doing a special promotion: Buy any five pieces of jewelry and receive the 6th one for free.""))
    
    
        
        
        
