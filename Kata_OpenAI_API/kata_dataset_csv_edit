def solve(arr):
    n = len(arr)
    dp = [0] * n
    for i in range(n):
        if arr[i] == 'C':
            dp[i] = 0
                dp[i] = 1 + dp[i-1]
                dp[i] = 1 + max(dp[j+1]+dp[j+2]+1 for j in range(0, i-2))
                pass
    return sum(dp)"
