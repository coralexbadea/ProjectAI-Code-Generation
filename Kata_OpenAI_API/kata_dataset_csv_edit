def solution(s):
    if len(s) % 2 == 0:
        return [s[i:i+2] for i in range(0, len(s), 2)]
    else:
        return [s[i:i+2] if i < len(s)-1 else s[-1]+'_' for i in range(0, len(s), 2)]"
