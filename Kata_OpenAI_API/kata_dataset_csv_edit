def solve(s):
    if len(s) == 0:
        return 0
    if len(s) % 2 != 0:
        return -1
    stack = []
    for i in range(len(s)):
        if s[i] == ')' and len(stack) > 0 and stack[-1] == '(':
            stack.pop()
            stack.append(s[i])"
