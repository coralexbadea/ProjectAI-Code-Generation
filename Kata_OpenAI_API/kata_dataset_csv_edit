def prefix_derivative(expression):
    if len(expression) == 1:
        return [1]
    result = []
    for i in range(len(expression) - 1, 0, -1):
        if expression[i].isdigit():
            continue
        elif expression[i] == '+':
            result.append('*')
        elif expression[i] == '-':
            result.append('*')"
