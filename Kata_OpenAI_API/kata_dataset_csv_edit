def ip_addresses(subnet):
    if not is_valid_ipv4(subnet):
        return None
    addresses = []
    for i in range(1, 4):
        for j in range(1, 4):
            for k in range(1, 4):
                for l in range(1, 4):
                    if i + j + k + l == len(subnet.split('.')):
                        address = '.'.join([str((int('0x'+i) * 256**3) + (int('0x'+j) * 256**2) + (int('0x'+k)*256)+ int('0x'+l)), subnet])
    return sorted([address[:-len(""."".join([str((int('0x'+i)*256**3),"
